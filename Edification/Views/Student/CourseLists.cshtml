@model IEnumerable<Course>
<div class="row">


    <div class="col-md-4"></div>

    <div class="col-md-6">
        <h1>Courses</h1>
        <br />
        <table class="table">
            <thead>
                <tr>

                    <th scope="col">Course Name</th>
                    <th scope="col">Course Fee</th>
                    <th scope="col">Course Percentage</th>
                </tr>
            </thead>
            <tbody>

                <tr>
                    @foreach (var item in Model)
                    {
                        <td>@item.CoursesName</td>

                        <td>@item.CoursesFees</td>

                        <td>@item.CoursesRequiredPercentage</td>
                    }
                </tr>

            </tbody>
        </table>

    </div>

</div>